<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kishorramani.navigationcomponentbasic">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NavigationComponentBasic">
        <activity android:name="com.kishorramani.navigationcomponentbasic.Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.kishorramani.navigationcomponentbasic.MainActivity" />
        <activity android:name="com.kishorramani.navigationcomponentbasic.MainActivity2" />
        <activity android:name="com.kishorramani.navigationcomponentbasic.MainActivity3" />
    </application>

</manifest>